{
  order forward_proxy before file_server
}
:443, pan.jdz.buzz {
  log {
	output file /var/log/caddy/access.log
  }
  tls jdzcn1@gmail.com
  forward_proxy {
    basic_auth user pass
    hide_ip
    hide_via
    probe_resistance
  }

  #reverse_proxy https://cloud.jdz.buzz
  root * /var/www/html
  file_server  
  php_fastcgi unix//run/php/php8.2-fpm.sock
}

